#include <stdio.h>

int main() {
    float salary;
    int creditScore, experience;
    printf("Enter Salary: ");
    scanf("%f", &salary);
    printf("Enter Credit Score: ");
    scanf("%d", &creditScore);
    printf("Enter Experience (Years): ");
    scanf("%d", &experience);
    if (salary >= 30000 && creditScore >= 750 && experience >= 2) {
        printf("Loan Approved\n");
    } else {
        printf("Loan Not Approved\n");
    }

    return 0;
}
